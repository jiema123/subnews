{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "server",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-06",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "kv_namespaces": [
    {
      "binding": "SUBNEWS_KV",
      "id": "SUBNEWS_KV_ID"
    }
  ],
  "triggers": {
    "crons": [
      "*/30 * * * *"
    ]
  }
}